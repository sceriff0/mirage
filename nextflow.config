params {
    input = './data/*.nd2'
    preproc_normalize = true
    preproc_tile_size = 512
    preproc_pool_workers = 4
    seg_model = 'cellpose'
    seg_gpu = true
}

profiles {
    slurm {
        process.executor = 'slurm'
        process.queue = 'normal'
    }
    gpu {
        process.label = 'gpu'
    }
}
